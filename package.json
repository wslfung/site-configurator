{
  "name": "site-configurator",
  "version": "0.2.0",
  "private": true,
  "main": "dist/electron/main.js",
  "author": "Wilson Fung <wilson.fung@ideaspark.ca>",
  "maintainers": ["Wilson Fung <wilson.fung@ideaspark.ca>"],
  "scripts": {
    "dev": "cross-env NODE_ENV=development next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "electron-dev": "tsc -p electron/tsconfig.json && concurrently \"npm run dev\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-build": "cross-env NODE_ENV=production next build && tsc -p electron/tsconfig.json && electron-builder"
  },
  "dependencies": {
    "@aws-sdk/client-codeartifact": "^3.787.0",
    "@aws-sdk/client-eventbridge": "^3.787.0",
    "@aws-sdk/client-lambda": "^3.787.0",
    "@aws-sdk/client-ses": "^3.787.0",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^7.0.2",
    "@mui/material": "^7.0.2",
    "@reduxjs/toolkit": "^2.2.1",
    "electron-is-dev": "^3.0.1",
    "electron-store": "^10.0.1",
    "next": "^15.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.51.0",
    "react-redux": "^9.1.0",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^35.1.5",
    "electron-builder": "^26.0.12",
    "eslint": "^8",
    "eslint-config-next": "14.1.3",
    "postcss": "^8",
    "postcss-import": "^16.1.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5",
    "wait-on": "^7.2.0"
  },
  "build": {
    "asar": true,
    "appId": "com.wilsonfung.site-configurator",
    "productName": "Site Configurator",
    "directories": {
      "output": "dist"
    },
    "files": [
      "dist/**/*",
      "package.json"
    ],
    "win": {
      "target": "nsis"
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}
